    "@rollup/plugin-typescript": "12.3.0",
    "@types/node": "18.19.130",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "@typescript-eslint/parser": "8.56.0",
    "babel-jest": "29.7.0",
    "eslint": "9.39.3",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "jest": "29.7.0",
    "npm-run-all2": "8.0.4",
